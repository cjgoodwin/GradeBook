grade = int(input("Enter your grade here: "))

if grade >=93 and grade <= 100:
  print ("For your grade of %d, your grade is an A." % (grade))
elif grade >= 90 and grade < 93:
  print ("For your grade of %d, your grade is an A-" % (grade))
elif grade >= 87 and grade < 90:
  print ("For your grade of %d, your grade is a B+" % (grade))
elif grade >= 83 and grade < 87:
  print ("For your grade of %d, your grade is a B" % (grade))
elif grade >= 80 and grade < 83:
  print ("For your grade of %d, your grade is a B-" % (grade))
elif grade >= 77 and grade < 80:
  print ("For your grade of %d, your grade is a C+" % (grade))
elif grade >= 73 and grade < 77:
  print ("For your grade of %d, your grade is a C" % (grade))
elif grade >= 70 and grade < 73:
  print ("For your grade of %d, your grade is a C-" % (grade))
elif grade >= 67 and grade < 70:
  print ("For your grade of %d, your grade is a D+" % (grade))
elif grade >= 63 and grade < 67:
  print ("For your grade of %d, your grade is a D" % (grade))
elif grade >= 60 and grade < 63:
  print ("For your grade of %d, your grade is a D-" % (grade))
elif grade < 60:
  print ("For your grade of %d, your grade is an F" % (grade))
else :
  print ("Oops, that's not a grade. Please try again.")
